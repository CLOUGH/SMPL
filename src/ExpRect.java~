import java.awt.*;

public class ExpRect extends Exp {
     Exp xcord;
     Exp ycord;
     Exp canvas;
    
     public ExpRect(Exp x, Exp y, Exp can)
     {
	xcord = x;
	ycord = y;
	canvas = can;
     }
     public Exp getX() {
	return xcord;
     }
     public Exp getY() {
	return ycord;
     }
     public Exp getCanvas() {
	return canvas;
     }

     public void paint(Graphics g) {
	    g.drawRect(10,10,20,40);	
     }
 
     public Object visit(Visitor v, Object arg) throws Exception
     {
	return v.visitExpRect(this, arg);
     }

     public String toString() {
     	return "Rectange created at xcord: "+xcord.toString()+" ycord: "+ycord.toString();
     }

     
}
